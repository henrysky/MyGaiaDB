CREATE TABLE twomass_psc (
    ra double precision,
    dec double precision,
    designation varchar,
    j_m real,
    j_snr real,
    h_m real,
    h_snr real,
    k_m real,
    k_snr real,
    ph_qual character(3),
    rd_flg character(3),
    bl_flg character(3),
    cc_flg character(3),
    ndet character(6),
    prox real
);

